{
  "name": "@poursha98/react-ios-time-picker",
  "version": "2.0.2",
  "description": "iOS-style wheel picker component for React with smooth scroll-snap, accessibility, 12-hour AM/PM support, and shadcn-style compound components for full customization.",
  "author": "Poursha98",
  "license": "MIT",
  "keywords": [
    "react",
    "wheel-picker",
    "ios",
    "picker",
    "scroll-picker",
    "time-picker",
    "date-picker",
    "accessible",
    "rtl",
    "persian",
    "farsi",
    "typescript",
    "mobile",
    "touch",
    "scroll-snap",
    "a11y",
    "component",
    "shadcn",
    "compound-components",
    "headless",
    "tailwind"
  ],
  "homepage": "https://github.com/Poursha98/react-ios-time-picker#readme",
  "bugs": {
    "url": "https://github.com/Poursha98/react-ios-time-picker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Poursha98/react-ios-time-picker.git"
  },
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "clsx": "^2.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0"
  },
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "npm run build"
  }
}
